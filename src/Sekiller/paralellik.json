        "onHover": {
          "enable": true,
          "mode": "grab",
          "parallax": {
            "enable": true,
            "force": 60,
            "smooth": 10
          }
        },
        "resize": true,
        "delay":0.5
      },
      "modes": {
        "push": {
          "quantity": 4
        },
        "repulse": {
          "distance": 200,
          "duration": 0.4
        }
      }
    },
    "particles": {
